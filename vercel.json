{
    "version": 2,
    "regions": [
        "sin1"
    ],
    "functions": {
        "api/translate.js": {
            "maxDuration": 120
        },
        "api/translate-stream.js": {
            "maxDuration": 120
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ]
}